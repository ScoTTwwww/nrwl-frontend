            "tsConfig": "libs/common/shared/tsconfig.spec.json",
            "setupFile": "libs/common/shared/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
